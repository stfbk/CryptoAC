<html>
    <head>
        <link rel="stylesheet"
              type="text/css"
              href="/assets/error.css" />
        <link rel="stylesheet"
              href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css"
              integrity="sha384-BVYiiSIFeK1dGmJRAkycuHAHRg32OmUcww7on3RYdg4Va+PmSTsz/K68vbdEjh4u"
              crossorigin="anonymous" />
        <link rel='stylesheet'
              href='https://fonts.googleapis.com/css?family=Anton|Passion+One|PT+Sans+Caption'
              type='text/css' />
        <link rel="icon" href="/assets/favicon.png" />

        <script type="text/javascript"
                src="https://code.jquery.com/jquery-3.3.1.min.js"
                integrity="sha256-FgpCb/KJQlLNfOu91ta32o/NMZxltwRo8QtmkMRdAu8="
                crossorigin="anonymous"></script>

        <script type="text/javascript" src="/assets/error.js"></script>

    </head>

    <body>
        <!-- Error Page -->
        <div class="error">
            <div class="container-fluid">
                <div class="col-xs-12 ground-color text-center">
                    <div class="container-error-404">
                        <div class="clip"><div class="shadow"><span class="digit thirdDigit" >$first</span></div></div>
                        <div class="clip"><div class="shadow"><span class="digit secondDigit">$second</span></div></div>
                        <div class="clip"><div class="shadow"><span class="digit firstDigit" >$third</span></div></div>
                        <div class="msg">OH!<span class="triangle"></span></div>
                    </div>
                    <h2 class="h1" id="errorMessage">$errorMessage</h2>
                </div>
            </div>
        </div>
        <!-- Error Page -->
    </body>

</html>